{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [false],
        "no-console": false,
        "max-line-length": [
            true,
            {
                "limit": 120,
                "ignore-pattern": "^import [^,]+ from |^export | implements | // || /*"
            }
        ],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}